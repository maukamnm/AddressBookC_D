package munoz.com;

import java.util.Scanner;

public class Driver {
	
	static AddBook ab = new AddBook();
	
	static Scanner sc = new Scanner(System.in);
	
	public static void main(String[]args) {
		System.out.println("Would you like to get started? Y for yes, N for no");
		String start = sc.next();
		if (start.contentEquals("y")||start.contentEquals("Y")) {
			ab.start();
		}
	}
}
